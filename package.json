    "@next/bundle-analyzer": "^15.4.4",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/react": "^16.3.0",
    "@types/d3": "^7.4.3",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "ora": "^8.2.0",
    "postcss": "^8.5.6",
